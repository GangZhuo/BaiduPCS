/*配置*/
{
	"cookieFilePath": "~/.baidupcs/default.cookie", /*使用的COOKIE*/
	"cacheFilePath": "", /*缓存文件的路径*/
	"logFilePath": "", /*日志文件的路径*/
	"items": [{
		"enable": 1,
		"localPath": "",
		"remotePath": "",
		"method": "update", /*针对该项执行的操作。值为：update - 更新远程文件的元数据到本地；backup - 执行备份；restore - 执行还原操作；reset - 重置本地缓存；combin - 合并本地和服务器*/
		"schedule": "06:00:00", /*几点几分开始执行任务 */
		"interval": 86400 /*任务执行完成后，当前执行时间加上interval则为下次执行时间。86400秒 = 24小时，即：当凌晨6点执行完任务后，加上24小时的时间作为任务下次执行时间。*/
	}]
}